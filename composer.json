{
    "name": "smallruraldog/admin",
    "description": ":description",
    "license": "license",
    "authors": [
        {
            "name": "author name",
            "email": "author email",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/smallruraldog/admin",
    "keywords": ["Laravel", "Admin"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SmallRuralDog\\Admin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SmallRuralDog\\Admin\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmallRuralDog\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "SmallRuralDog\\Admin\\Facades\\Admin"
            }
        }
    }
}
